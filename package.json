{
  "name": "pinterval",
  "version": "1.0.0",
  "description": "Advanced interval",
  "main": "lib/index.js",
  "scripts": {
    "gh-pages": "git subtree push --prefix docs origin gh-pages",
    "clean": "rimraf lib",
    "build": "cross-env NODE_ENV=production npm run clean && tsc -b tsconfig.json",
    "test": "rimraf coverage && cross-env NODE_ENV=test nyc mocha",
    "coveralls": "npm test && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "doc": "rimraf docs && typedoc --excludeProtected --excludePrivate --out docs && touch docs/.nojekyll",
    "fmt": "pretty-quick"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ziflex/pinterval.git"
  },
  "keywords": [
    "interval",
    "setInterval"
  ],
  "author": "Tim Voronov <ziflex@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ziflex/pinterval/issues"
  },
  "homepage": "https://github.com/ziflex/pinterval#readme",
  "dependencies": {
    "is-function": "^1.0.1",
    "is-promise": "^2.1.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.7",
    "@types/nanoid": "^2.0.0",
    "@types/node": "^12.7.0",
    "@types/sinon": "^7.0.13",
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "husky": "^3.0.2",
    "mocha": "^6.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "rimraf": "^2.6.3",
    "sinon": "^7.4.1",
    "source-map-support": "^0.5.13",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.5.3"
  }
}
